     name : VerifierPorts
     procedure_id : 1787877585475817279
     type_code : 14
     code : |1-
      // Résumé : <indiquez ici ce que fait la procédure>
      // Syntaxe :
      //[ <Résultat> = ] VerifierPorts (<sListePort> est chaîne)
      //
      // Paramètres :
      //	sListePort (chaîne) : <indiquez ici le rôle de sListePort>
      // Valeur de retour :
      // 	booléen : // 	Aucune
      //
      // Exemple :
      // Indiquez ici un exemple d'utilisation.
      //
      Procédure VerifierPorts(sListePort est une chaine)
      
      // Variable
      sPort est une chaine
      
      // Chaine vide, aucun port n'est filtré
      SI sListePort = "" RENVOYER Vrai
      
      POUR TOUTE CHAINE sPort DE sListePort SEPAREE PAR ";"
      	// Si Val = 0 c'est que ce n'est pas un nombre qui a été inséré
      	SI Val(sPort) = 0 ALORS
      		Renvoyer Faux
      	FIN
      FIN
      
      RENVOYER Vrai

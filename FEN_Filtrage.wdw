     height : 24
    type : 4
    code_elements :
     type_code : 4
     p_codes :
      -
        code : |1+
         
         // IGMP filtré ou non ?
         SI INT_OptionsFiltrage[1]..Valeur = Vrai ALORS
         	clMainCapture:CapturerIGMP(Vrai)
         SINON
         	clMainCapture:CapturerIGMP(Faux)
         FIN
         
         // ICMP filtré ou non ?
         SI INT_OptionsFiltrage[2]..Valeur = Vrai ALORS
         	clMainCapture:CapturerICMP(Vrai)
         SINON
         	clMainCapture:CapturerICMP(Faux)
         FIN
         
         // UDP et TCP filtrés ou non ? Si on les filtre, on fournit les ports souhaitées en paramètre pour la capture
         // Les doivent être séparés par un ;
         SI INT_OptionsFiltrage[3]..Valeur = Vrai ALORS
         	SI VerifierPorts(SAI_PortUDP) ALORS
         		clMainCapture:CapturerUDP(Vrai,SAI_PortUDP)
         	SINON
         		Erreur(<§$0001§>+RC+RC+<§$0003§>+RC+RC+<§$0006§>)
         		RETOUR
         	FIN
         SINON
         	clMainCapture:CapturerUDP(Faux)
         FIN
         
         SI INT_OptionsFiltrage[4]..Valeur = Vrai ALORS
         	SI VerifierPorts(SAI_PortTCP) ALORS
         		clMainCapture:CapturerTCP(Vrai,SAI_PortTCP)
         	SINON
         		Erreur(<§$0002§>+RC+RC+<§$0004§>+RC+RC+<§$0005§>)
         		RETOUR
         	FIN
         SINON
         	clMainCapture:CapturerTCP(Faux)
         FIN
         
         // Sauvegarde des paramètres
         SauveParamètre("IGMP", INT_OptionsFiltrage[1]..Valeur)
         SauveParamètre("ICMP", INT_OptionsFiltrage[2]..Valeur)
         SauveParamètre("UDP", INT_OptionsFiltrage[3]..Valeur)
         SauveParamètre("TCP", INT_OptionsFiltrage[4]..Valeur)
         SauveParamètre("PortUDP", SAI_PortUDP)
         SauveParamètre("PortTCP", SAI_PortTCP)
         
         // Fermeture de la fenêtre
         Ferme(FEN_Filtrage)
